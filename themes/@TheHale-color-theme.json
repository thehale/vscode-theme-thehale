{
	"name": "@TheHale",
	"colors": {
		// Code Editor Colors
		"editor.background": "#1f1f1f",
		"editor.foreground": "#eeffff",
		"editor.selectionForeground": "#ffffff44",
		"editor.selectionBackground": "#002fff44",
		"editor.inactiveSelectionBackground": "#002fff22",
		"editor.selectionHighlightBackground": "#ffffff05",
		"editor.selectionHighlightBorder": "#616161cc",
		"editor.wordHighlightBackground": "#ffffff05",
		"editor.wordHighlightBorder": "#616161cc",
		"editor.wordHighlightStrongBackground": "#ffffff05",
		"editor.wordHighlightStrongBorder": "#616161cc",
		"editor.rangeHighlightBackground": "#ffffff05",
		"editor.rangeHighlightBorder": "#616161cc",
		"editor.findMatchBackground": "#ffffff11",
		"editor.findMatchBorder": "#616161cc",
		"editor.findMatchHighlightBackground": "#ffffff05",
		"editor.findMatchHighlightBorder": "#616161cc",
		"editor.findRangeHighlightBackground": "#ffffff05",
		"editor.findRangeHighlightBorder": "#616161cc",
