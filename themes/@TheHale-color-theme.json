    "semanticHighlighting": true,
    "semanticTokenColors": {
        "keyword": "#CC7832",
        "keyword.documentation": {
            "foreground": "#808080",
            "fontStyle": "bold"
        },
        "comment": "#EC407A",
        "string": "#a571ff",
        "namespace": {
            "foreground": "#0692f0",
            "fontStyle": "italic"
        },
        "annotation": "#AFC367",
        "class": {
            "foreground": "#0692f0",
            "fontStyle": "bold"
        },
        "class.decorator": "#AFC367",
        "interface": "#3C9CFF",
        "enum": {
            "foreground": "#0692f0",
            "fontStyle": "italic bold"
        },
        "enumMember": {
            "foreground": "#4BD5F8",
            "fontStyle": "italic bold"
        },
        "function": {
            "foreground": "#02d421",
            "fontStyle": ""
        },
        "function.static": {
            "foreground": "#02d421",
            "fontStyle": "italic"
        },
        "function.decorator": {
            "foreground": "#AFC367",
            "fontStyle": ""
        },
        "magicFunction": {
            "foreground": "#BA68C8",
            "fontStyle": ""
        },
        "intrinsic": {
            "foreground": "#BA68C8",
            "fontStyle": ""
        },
        "method": {
            "foreground": "#02d421",
            "fontStyle": ""
        },
        "method.static": {
            "foreground": "#02d421",
            "fontStyle": "italic"
        },
        "variable": "#BAB995",
        "variable.decorator": "#AFC367",
        "parameter": "#C6B800",
        "parameter.documentation": {
            "foreground": "#EC407A",
            "fontStyle": "italic"
        },
        "property": {
            "foreground": "#4BD5F8",
            "fontStyle": ""
        },
        "property.static": {
            "foreground": "#4BD5F8",
            "fontStyle": "italic"
        },
        "selfParameter": {
            "foreground": "#BA68C8",
            "fontStyle": ""
        },
        "clsParameter": {
            "foreground": "#BA68C8",
            "fontStyle": ""
        }
